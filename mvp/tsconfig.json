{
  "files": [],
  "references": [
    {
      "path": "./tsconfig.app.json"
    },
    {
      "path": "./tsconfig.node.json"
    }
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@/app/*": [
        "./src/app/*"
      ],
      "@/assets/*": [
        "./src/assets/*"
      ],
      "@/components/*": [
        "./src/components/*"
      ],
      "@/features/*": [
        "./src/features/*"
      ],
      "@/modules/*": [
        "./src/modules/*"
      ],
      "@/pages/*": [
        "./src/pages/*"
      ],
      "@/services/*": [
        "./src/services/*"
      ],
      "@/hooks/*": [
        "./src/hooks/*"
      ],
      "@/routes/*": [
        "./src/routes/*"
      ],
      "@/shared/*": [
        "./src/shared/*"
      ],
      "@/styles/*": [
        "./src/styles/*"
      ],
      "@/lib/*": [
        "./src/lib/*"
      ]
    },
    "noImplicitAny": false,
    "noUnusedParameters": false,
    "skipLibCheck": true,
    "allowJs": true,
    "noUnusedLocals": false,
    "strictNullChecks": false
  }
}